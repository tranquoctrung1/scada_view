extends ./partial/layout.pug
block linkcss
    link(rel="stylesheet", href="/css/dashboardVilog.css")
    link(rel="stylesheet", href="/css/pageCommon.css")
block content
    .container-fluid
        .row.title-pressure
            h3 Bảng Thống Kê Tình Trạng Hoạt Động Solar Vilog
        .row.result
            .col-12 
                .row.chart-type-error
                    h6.text-center.d-center Biểu đồ bánh thể hiện số lượng trạng thái của Vilog
                    #chartTypeError
            .col-12
                .row.table-data.d-center
                    h6.text-center.d-center Bảng số liệu của Vilog 
                    #tableData
            .col-12
                .row.chart-V_Acc
                    h6.text-center.d-center Biểu đồ cột thể hiện trung bình điện áp Solar của các điểm đo
                    #chartVAcc
            .col-12
                .row.chart-V_Acc
                    h6.text-center.d-center Biểu đồ cột thể hiện trung bình điện áp Logger của các điểm đo
                    #chartVLogger
            
            .col-12
                .row.chart-V_Acc
                    h6.text-center.d-center Biểu đồ cột thể hiện trung bình điện áp đồng hồ của các điểm đo
                    #chartVMetter
            
block linkcriptbody
    script(src="/js/hostname.js") 
    script(src="/js/dashboardVilog.js") 
    script(src="/js/util.js") 